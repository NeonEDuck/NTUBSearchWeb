<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="北商大查詢教學評量填答率">
  <meta name="keyword" content="北商,北商大,填答,北商教學評量,北商大教學評量,北商大查詢教學評量填答率">
  <link href="./images/ntub.png" rel="shortcut icon" >
  <title>北商大查詢教學評量填答率</title>

  <!-- Bootstrap core CSS -->
  <link href="./vendors/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom fonts for this template -->
  <link href="./vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="./stylesheets/grayscale.min.css" rel="stylesheet">
  <link href="./stylesheets/main.css" rel="stylesheet">

</head>

<body id="page-top">

  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
    <div class="container">
      <a class="navbar-brand js-scroll-trigger" href="#page-top">查詢教學評量</a>
      
    </div>
  </nav>

  <!-- Header -->
  <header class="masthead">
    <div class="container d-flex h-100 align-items-center">
      <div class="mx-auto text-center">
        <h1 class="mx-auto my-0 text-uppercase">NTUB</h1><p><p><p></p><p></p>

        <div class="dropdown">
          <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" margin-top=60px>
            <% if ( type != null ) { %>
                <%= type %>
            <% } else { %>
                學生科系
            <% } %>
          </button>
          
          <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
              
            <% data.forEach(function(d){ %>
                <a class="dropdown-item" href="type=<%= d %>"><%= d %></a>
            <% }); %>

          </div>
        </div>

        <div class="dropdown">
            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                
                <% if ( clas != null ) { %>
                    <%= clas %>
                <% } else if ( type != null ) { %>
                    班級
                <% } else { %>
                    
                <% } %>
            </button>
            
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <% if (clasList != null) { %>
                    <% clasList.forEach(function(c){ %>
                        <a class="dropdown-item" href="type=<%= type %>&class=<%= c %>"><%= c %></a>
                    <% }); %>
                <% } %>
  
            </div>
          </div>

        <p><br>
        <h2 class="text-white-50 mx-auto mt-2 mb-5">未填答完成之教學評量之學生名單<p></br></p></h2>
        <!-- <a href="#about" class="btn btn-primary js-scroll-trigger">查詢</a> -->
      </div>
    </div>
  </header>

    <div class="bg-black" style="display: flex;justify-content:center;align-items:center;">
        <% if ( table != null ){ %>
            <table class="bg-black" style="width:80vw; background-color: #444; color:#fff; ">
                <tr>
                    <th width="33%">學生班級</th>
                    <th width="33%">學號</th>
                    <th width="33%">學生姓名</th>
                </tr>
                <% table.forEach(function(row){ %>
                    <tr>
                        <th><%= row.class %></th>
                        <th><%= row.no %></th>
                        <th><%= row.name %></th>
                    </tr>
                <% }); %>
            </table>
        <% } %>
    </div>

  <!-- Footer -->
  <footer class="bg-black small text-center text-white-50">
    <div class="container">
      Copyright &copy; NTUB IMD 2019
    </div>
  </footer>

  <!-- Bootstrap core JavaScript -->
  <script src="./vendors/jquery/jquery.min.js"></script>
  <script src="./vendors/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Plugin JavaScript -->
  <script src="./vendors/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for this template -->
  <script src="./javascripts/grayscale.min.js"></script>
  
</body>

</html>